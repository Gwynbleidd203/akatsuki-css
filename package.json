{
  "name": "akatsuki-css",
  "version": "1.0.0",
  "description": "A simple front-end framework.",
  "main": "index.js",
  "dependencies": {
    "anymatch": "^3.1.3",
    "binary-extensions": "^2.2.0",
    "braces": "^3.0.2",
    "chokidar": "^3.5.3",
    "fill-range": "^7.0.1",
    "glob-parent": "^5.1.2",
    "immutable": "^4.3.0",
    "is-binary-path": "^2.1.0",
    "is-extglob": "^2.1.1",
    "is-glob": "^4.0.3",
    "is-number": "^7.0.0",
    "normalize-path": "^3.0.0",
    "picomatch": "^2.3.1",
    "readdirp": "^3.6.0",
    "source-map-js": "^1.0.2",
    "to-regex-range": "^5.0.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:sass": "sass src/sass/main.scss src/css/style.css -w",
    "compile:sass": "sass src/sass/main.scss src/css/style.comp.css",
    "concat:css": "concat -o src/css/style.concat.css src/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/css/style.concat.css -o src/css/style.prefix.css",
    "buidl:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "keywords": [
    "css",
    "sass",
    "front-end"
  ],
  "author": "vattghern203",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "sass": "^1.59.3"
  }
}
